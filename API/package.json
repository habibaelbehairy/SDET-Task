  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@jest/globals": "^30.0.4",
    "express": "^5.1.0",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "mock-user-auth": "^1.0.15",
    "supertest": "^6.3.4"
  }
}
