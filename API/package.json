{
  "name": "auth-api-testing",
  "version": "1.0.0",
  "description": "Testing mock-user-auth API",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./node_modules/mock-user-auth/bin/www.js",
    "start": "node ./node_modules/mock-user-auth/bin/www.js",
    "test": "jest --config jest.config.js --detectOpenHandles --forceExit",
    "test:auth": "jest tests/auth --config jest.config.js --detectOpenHandles --forceExit",
    "test:users": "jest tests/users --config jest.config.js --detectOpenHandles --forceExit",
    "test:report": "jest --config jest.config.js --detectOpenHandles --forceExit && start reports/test-report.html"
  },
  "keywords": [],
